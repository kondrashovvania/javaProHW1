public class Person {
    private String fullName;
    private int age;

    public Person() {
    }

    public Person(String fullName, int age) {
        this.fullName = fullName;
        this.age = age;
    }

    public void move() {
        System.out.println(fullName + " говорит.");
    }

    public void talk() {
        System.out.println(fullName + " говорит.");
    }

    public static void main(String[] args) {
        Person person1 = new Person();
        person1.fullName = "Иван Иванов";
        person1.age = 25;
        person1.move(); // Вывод: Иван Иванов говорит.
        person1.talk(); // Вывод: Иван Иванов говорит.

        Person person2 = new Person("Петр Петров", 30);
        person2.move(); // Вывод: Петр Петров говорит.
        person2.talk(); // Вывод: Петр Петров говорит.
    }
}
